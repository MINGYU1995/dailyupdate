        <RadioButton x:Name="radi1" Content="" Margin="0,33,23,0" RenderTransformOrigin="-0.264,-6.858" Checked="RadioButton_Checked" MinWidth="2" Grid.Row="2" Grid.Column="3"/>
        <Label Content="충전대" Margin="2,22,2,25" RenderTransformOrigin="-0.158,0.22" FontSize="16" Grid.Row="2" Grid.Column="2" Grid.RowSpan="2"/>


        <RadioButton x:Name="radi2" Content="" Margin="0,34,23,0" RenderTransformOrigin="-0.264,-6.858" Checked="RadioButton_Checked" Grid.Row="2" Grid.Column="6"/>
        <Label Content="배출대" Margin="2,22,41,25" RenderTransformOrigin="-0.158,0.22" FontSize="16" Grid.Row="2" Grid.Column="5" Grid.RowSpan="2" Grid.ColumnSpan="2"/>


        <RadioButton x:Name="radi3" Content="" Margin="0,33,19,0" RenderTransformOrigin="-0.264,-6.858" Checked="RadioButton_Checked" Grid.Row="2" Grid.Column="9"/>
        <Label Content="복귀중" Margin="3,22,0,25" RenderTransformOrigin="-0.158,0.22" FontSize="16" Grid.Row="2" Grid.Column="8" Grid.RowSpan="2"/>


        <TextBox x:Name="move1" Height="23" Width="62" Margin="2,29,2,0" TextWrapping="Wrap" RenderTransformOrigin="0.638,1.152" TextChanged="TextBox_TextChanged_1" SelectionOpacity="0.1" SpellCheck.IsEnabled="True" VerticalAlignment="Top" Text="11.25" Grid.Row="4" Grid.Column="2" HorizontalContentAlignment="Right"/>
        <TextBox x:Name="move2" Height="23"  Width="62" Margin="1,29,2,0" TextWrapping="Wrap" RenderTransformOrigin="0.638,1.152" SelectionOpacity="0.1" SpellCheck.IsEnabled="True" VerticalAlignment="Top" Grid.Row="4" Grid.Column="5" HorizontalContentAlignment="Right"/>
        <TextBox x:Name="move3" Height="23" Width="62" Margin="1,29,2,0" TextWrapping="Wrap" RenderTransformOrigin="0.638,1.152" SpellCheck.IsEnabled="True" SelectionOpacity="0.1" Grid.Row="4" Grid.Column="8" HorizontalContentAlignment="Right"/>
        <TextBox x:Name="move4" Width="62"  Margin="6,30,0,0" TextWrapping="Wrap" RenderTransformOrigin="0.638,1.152" TextChanged="TextBox_TextChanged" SelectionOpacity="0.1" SpellCheck.IsEnabled="True" HorizontalAlignment="Left" Text="120" Grid.Row="6" Grid.Column="5" Height="23" VerticalAlignment="Top" Grid.ColumnSpan="2" HorizontalContentAlignment="Right"/>
        <TextBox x:Name="move5" Height="23" Width="62" Margin="1,29,2,0" TextWrapping="Wrap" RenderTransformOrigin="0.638,1.152" TextChanged="TextBox_TextChanged" SelectionOpacity="0.1" VerticalAlignment="Top" Text="62.5" Grid.Row="8" Grid.Column="5" HorizontalContentAlignment="Right"/>
        <TextBox x:Name="move6" Height="23"  Width="62" Margin="6,29,0,0" TextWrapping="Wrap"  RenderTransformOrigin="0.638,1.152" TextChanged="TextBox_TextChanged" SelectionOpacity="0.1" HorizontalAlignment="Left" VerticalAlignment="Top" Text="120" Grid.Row="9" Grid.Column="5" Grid.ColumnSpan="2" HorizontalContentAlignment="Right"/>



        <Label x:Name="msg_Copy2" Content="이동" Margin="10,25,0,0" Width="37" Height="30" RenderTransformOrigin="0.847,1.386" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <Label x:Name="msg_Copy4" Content="s" Margin="0,29,0,0" Width="21" Height="25" RenderTransformOrigin="-2.21,-0.944" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4" Grid.Column="9" Grid.RowSpan="2"/>
        <Label x:Name="msg_Copy3" Content="s" Margin="0,29,0,0" Width="19" Height="23" RenderTransformOrigin="-2.21,-0.944" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4" Grid.Column="6"/>
        <Label Content="수거" Margin="0,26,0,0" Width="47" Height="30" RenderTransformOrigin="0.847,1.386" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="6" Grid.Column="4" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <Label x:Name="msg_Copy7" Content="배출대 이동" Margin="11,27,0,0" Width="78" Height="30" RenderTransformOrigin="0.847,1.386" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="8" Grid.ColumnSpan="3" Grid.Column="3"/>



        <Label x:Name="msg_Copy12" Content="배출" Margin="0,27,0,0" Width="47" Height="30" RenderTransformOrigin="0.847,1.386" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="9" Grid.Column="4" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <Label x:Name="msg_Copy26" Content="s" Margin="0,29,0,0" Width="24" Height="23" RenderTransformOrigin="-2.21,-0.944" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="8" Grid.Column="6"/>
        <Label x:Name="msg_Copy27" Content="s" Margin="1,29,0,0" Width="18" Height="23" RenderTransformOrigin="-2.21,-0.944" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4" Grid.Column="3"/>
        <Label x:Name="msg_Copy28" Content="s" Margin="0,29,0,0" Width="20" Height="23" RenderTransformOrigin="-2.21,-0.944" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="9" Grid.Column="6"/>
        <Label x:Name="msg_Copy" Content="s" Margin="0,29,0,0" Width="20" Height="23" RenderTransformOrigin="-2.21,-0.944" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="6" Grid.Column="6"/>
    </Grid>
</UserControl>
