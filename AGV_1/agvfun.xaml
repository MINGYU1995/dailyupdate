            <ColumnDefinition Width="45*"/>
            <ColumnDefinition Width="46*"/>
            <ColumnDefinition Width="66*"/>
            <ColumnDefinition Width="42*"/>
            <ColumnDefinition Width="42*"/>
            <ColumnDefinition Width="66*"/>
            <ColumnDefinition Width="42*"/>
            <ColumnDefinition Width="45*"/>
            <ColumnDefinition Width="66*"/>
            <ColumnDefinition Width="42*"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="53.846"/>
            <RowDefinition Height="26.154"/>
            <RowDefinition Height="47.692"/>
            <RowDefinition Height="32.308"/>
            <RowDefinition Height="52.308"/>
            <RowDefinition Height="27.692"/>
            <RowDefinition Height="53.077"/>
            <RowDefinition Height="26.923"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="53"/>
            <RowDefinition Height="27.615"/>
        </Grid.RowDefinitions>

        <Border x:Name="point_1" BorderBrush="Black" BorderThickness="1" Width="20" Height="16" Margin="16,38,0,0" Background="#FFFBFBFB" RenderTransformOrigin="2.962,-3.131" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Column="0"/>
        <Border x:Name="point_2" BorderBrush="Black" BorderThickness="1" Width="20" Height="16" Margin="16,32,0,0" Background="#FFF9FBF7" RenderTransformOrigin="2.962,-3.131" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="2"/>
        <Border x:Name="point_3" BorderBrush="Black" BorderThickness="1" Width="20" Height="16" Margin="16,36,0,0" Background="White" RenderTransformOrigin="2.962,-3.131" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4"/>
        <Border x:Name="point_4" BorderBrush="Black" BorderThickness="1" Width="20" Height="16" Margin="16,37,0,0"  Background="White" RenderTransformOrigin="2.962,-3.131" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="6"/>
        <Border x:Name="point_5" BorderBrush="Black" BorderThickness="1" Width="20" Height="16" Margin="16,33,0,0"  Background="White" RenderTransformOrigin="2.962,-3.131" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="8"/>
        <Border x:Name="point_6" BorderBrush="Black" BorderThickness="1" Width="20" Height="16" Margin="16,33,0,0"  Background="#FFFEFFFD" RenderTransformOrigin="2.962,-3.131" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="9"/>



